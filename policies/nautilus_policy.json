{
  "allow_network": false,
  "readable_paths": [
    "/tmp",
    "/proc",
    "{mount_point}/nautilus_data"
  ],
  "writable_paths": [
    "{mount_point}/nautilus_data"
  ],
  "executable_paths": [
    "{mount_point}/nautilus_data"
  ],
  "restricted_paths": [
  ],
  "bubblewrap_params": [
    "--new-session",
    "--dev-bind", "/dev", "/dev",
    "--proc", "/proc",
    "--dir", "/tmp",
    "--bind", "{mount_point}", "/",
    "--chdir", "{mount_point}",
    "--bind", "{mount_point}/nautilus_data", "{mount_point}/nautilus_data"
  ]
}
