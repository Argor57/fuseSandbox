{
  "allow_network": true,
  "readable_paths": [
    "/etc/ssl",
    "/usr/lib/firefox",
    "/usr/lib/x86_64-linux-gnu",
    "/lib/x86_64-linux-gnu",
    "/usr/share/fonts",
    "/usr/share/icons",
    "/usr/share/mime",
    "/usr/share/glib-2.0",
    "/usr/share/pixmaps",
    "/usr/share/themes",
    "/usr/share/X11"
  ], 
  "writable_paths": [
    "/tmp",
    "/var/tmp",
    "/home",
    "/home/{username}/.mozilla"
  ],
  "executable_paths": [
    "/usr/lib/firefox/firefox",
    "/usr/lib/firefox/firefox-bin",
    "/usr/bin"
  ],
  "restricted_paths": [
  ],
  "bubblewrap_params": [
    "--new-session",
    "--clearenv",
    "--unshare-pid",
    "--unshare-uts",
    "--unshare-ipc",
    "--unshare-cgroup",
    "--unshare-user",
    "--chdir", "/home"
  ]
}
